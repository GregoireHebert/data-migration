{
    "name": "gheb/data-migrations-bundle",
    "description": "Migrate your data along side your data structure.",
    "type": "symfony-bundle",
    "keywords": ["DBAL", "Migrations", "data"],
    "license": "MIT",
    "authors": [
      {
        "name": "Grégoire Hébert",
        "email": "gregoire@les-tilleuls.coop"
      }
    ],
    "require": {
        "php": ">=7.1.0",
        "symfony/console": "~4.1",
        "symfony/config": "~4.1",
        "symfony/http-kernel": "~4.1",
        "symfony/dependency-injection": "~4.1",
        "doctrine/common": "^2.8",
        "doctrine/orm": "^2.6",
        "doctrine/migrations": "^2.0@beta",
        "doctrine/dbal": "^2.6",
        "doctrine/doctrine-bundle": "~1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^7.4"
    },
    "suggest": {
        "doctrine/doctrine-migrations-bundle": "Help you version the changes in your database schema"
    },
    "autoload": {
      "psr-4": { "Gheb\\DataMigrationsBundle\\": "src/" }
    },
    "autoload-dev": {
      "psr-4": { "Gheb\\DataMigrationsBundle\\Tests\\": "tests/" }
    },
    "config": {
      "sort-packages": true
    },
    "extra": {
      "branch-alias": {
        "dev-master": "2.0.x-dev"
      }
    },
    "minimum-stability": "beta"
}
