<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="data_migrations.execute_command" class="Gheb\DataMigrationsBundle\Command\MigrationsExecuteDoctrineCommand">
            <tag name="console.command" />
        </service>
        <service id="data_migrations.generate_command" class="Gheb\DataMigrationsBundle\Command\MigrationsGenerateDoctrineCommand">
            <tag name="console.command" />
        </service>
        <service id="data_migrations.latest_command" class="Gheb\DataMigrationsBundle\Command\MigrationsLatestDoctrineCommand">
            <tag name="console.command" />
        </service>
        <service id="data_migrations.migrate_command" class="Gheb\DataMigrationsBundle\Command\MigrationsMigrateDoctrineCommand">
            <tag name="console.command" />
        </service>
        <service id="data_migrations.status_command" class="Gheb\DataMigrationsBundle\Command\MigrationsStatusDoctrineCommand">
            <tag name="console.command" />
        </service>
        <service id="data_migrations.version_command" class="Gheb\DataMigrationsBundle\Command\MigrationsVersionDoctrineCommand">
            <tag name="console.command" />
        </service>
    </services>
</container>
